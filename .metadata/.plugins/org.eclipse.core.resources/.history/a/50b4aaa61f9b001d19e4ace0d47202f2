package unsolved.category.gold4.Shortest_Paths_With_Conditions_BOJ1504_;

public class Main {

	public static void main(String[] args) {
		StringBuilder sb = new StringBuilder();
		char[] abcd = { 'a', 'b', 'c', 'd' };
		for (int i = 0; i < 4; i++) {
			sb.append(abcd[i]);
		}
		System.out.println(sb);

	}

}
